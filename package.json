{
  "name": "versatiles-frontend",
  "version": "3.4.0",
  "private": true,
  "description": "",
  "bin": {
    "versatiles-frontend": "src/index.ts"
  },
  "scripts": {
    "build": "tsx --env-file-if-exists=.env src/build.ts",
    "check": "npm run lint && npm run test",
    "dev": "tsx --env-file-if-exists=.env src/dev.ts",
    "doc": "npm run doc-graph",
    "doc-graph": "vrt deps-graph | vrt doc-insert README.md '### Dependency Graph'",
    "lint": "eslint . --color",
    "prepack": "npm run build",
    "release": "vrt release-npm",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "upgrade": "vrt deps-upgrade"
  },
  "type": "module",
  "author": "Michael Kreil <versatiles@michael-kreil.de>",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@versatiles/style": "^5.7.0",
    "arg": "^5.0.2",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "ignore": "^7.0.5",
    "maplibre-gl": "^5.7.0",
    "mrmime": "^2.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-import-css": "^4.0.2",
    "supports-color": "^10.2.0",
    "tar": "^7.4.3",
    "tar-stream": "^3.1.7",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/tar-stream": "^3.1.4",
    "@types/unzipper": "^0.10.11",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@versatiles/release-tool": "^2.4.3",
    "eslint": "^9.34.0",
    "jest": "^30.1.2",
    "jest-extended": "^6.0.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
