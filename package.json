{
  "name": "versatiles-frontend",
  "version": "2.0.3",
  "private": true,
  "description": "",
  "bin": {
    "versatiles-frontend": "src/index.ts"
  },
  "scripts": {
    "build": "tsx src/build.ts",
    "check": "npm run lint && npm run test",
    "dev": "tsx src/dev.ts",
    "lint": "eslint . --color",
    "release": "vrt release-npm",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "upgrade": "npm-check-updates -u && rm -f package-lock.json; rm -rf node_modules; npm i && npm update"
  },
  "type": "module",
  "author": "Michael Kreil <versatiles@michael-kreil.de>",
  "license": "MIT",
  "dependencies": {
    "classic-level": "^1.4.1",
    "express": "^4.18.3",
    "ignore": "^5.3.1",
    "supports-color": "^9.4.0",
    "tar-stream": "^3.1.7",
    "tsx": "^4.7.1",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/tar-stream": "^3.1.3",
    "@types/unzipper": "^0.10.9",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@versatiles/release-tool": "^1.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "mrmime": "^2.0.0",
    "npm-check-updates": "^16.14.15",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
